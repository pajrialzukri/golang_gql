type User {
  id: ID!
  email: String!
  phone: String!
  otp: String!
  source: String!
}

input UserInput {
  id: ID!
  email: String!
  phone: String!
  otp: String!
  source: String!
  gender: String!
}

extend type Query {
  userGet(id: ID): [User]
}

extend type Mutation {
  userCreate(data: UserInput!): User!
}
